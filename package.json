{
  "name": "ensemble-api",
  "version": "1.2.5",
  "description": "The Ensemble API",
  "main": "dist/app.js",
  "author": "judehunter <matisowagm@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "ts-node --files src/app",
    "dev:drop": "cross-env DROP_DB=true yarn dev",
    "build": "tsc",
    "start": "node dist/app.js",
    "seed:all": "ts-node --files src/seeder/seeder",
    "lint": "eslint src/ --ext .ts",
    "deploy": "serverless deploy",
    "deploy:credentials": "serverless config credentials",
    "migration:create": "typeorm migration:create -d src/migrations -n",
    "migrate:up": "cross-env TS_NODE_FILES=true TYPEORM_MIGRATIONS=\"src/migrations/*.ts\" ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate:gen": "cross-env TS_NODE_FILES=true node -r ts-node/register ./node_modules/typeorm/cli.js migration:generate -p -n"
  },
  "devDependencies": {
    "@types/koa": "^2.11.4",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-jude": "^1.1.4",
    "nodemon": "^2.0.4",
    "ts-essentials": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/multer": "^3.0.0",
    "argon2": "^0.27.0",
    "axios": "^0.20.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "geojson": "^0.5.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^9.4.0",
    "pg": "^8.3.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.0-rc.21",
    "wkx": "^0.5.0"
  }
}
